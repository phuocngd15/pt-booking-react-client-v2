    "rc-virtual-list": "^3.4.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-highlight-words": "^0.20.0",
    "react-intl": "^6.2.7",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.0",
    "react-slick": "^0.29.0",
    "redux-persist": "^6.0.0",
    "slick-carousel": "^1.8.1",
    "sort-by": "^1.2.0",
    "tailwindcss": "^3.3.1"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.10.5",
    "@jsxiaosi/commitlint-config": "^0.0.14",
    "@jsxiaosi/eslint-config-prettier": "^0.0.14",
